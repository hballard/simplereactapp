{
  "name": "simplereactapp",
  "version": "1.0.0",
  "description": "A simple react app to learn react, flask, redux, graphene, and apollo-client",
  "scripts": {
    "start": "parallelshell 'npm run watch' 'npm run api'",
    "watch": "webpack-dev-server --progress --inline --hot --content-base static/",
    "build": "webpack -p",
    "api": "source ~/.virtualenvs/simplereactapp/bin/activate && python api.py"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "parallelshell": "^2.0.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "apollo-client": "^0.5.4",
    "graphql-tag": "^0.1.14",
    "lodash": "^4.16.4",
    "react": "^15.1.0",
    "react-apollo": "^0.6.0",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-redux-form": "^1.2.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
